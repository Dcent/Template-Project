{
    "version": "1.0.0",
    "name": "asp.net",
    "private": true,
    "description": "Template Project",
    "main": "dist/index.cjs.js",
    "module": "dist/index.mjs",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/stencil/stencil.esm.js",
    "files": [
        "dist/"
    ],
    "scripts": {
        "create:ts:class": "cd ./Frontend/scripts/src && hygen ts class",
        "create:ts:enum": "cd ./Frontend/scripts/src && hygen ts enum",
        "create:ts:util": "cd ./Frontend/scripts/src && hygen ts util",
        "create:ts:interface": "cd ./frontend/scripts/src && hygen ts interface",
        "create:ts:service": "cd ./frontend/scripts/src && hygen ts service",
        "create:ts:component": "cd ./frontend/scripts/src && hygen ts component",
        "static": "stencil build --dev --watch --static",
        "static:storybook": "concurrently --names \"STORYBOOK,STATIC,STYLING\" --prefix-colors \"yellow.bgMagenta.bold,cyan.bgBlue.bold,red.bgRed.bold\" \"npm run storybook\" \"npm run static\" \"npm run static:styling\"",
        "static:styling": "npx tailwindcss -i ./Frontend/styling/app.css -o ./Frontend/storybook/dist/css/app.css --watch ",
        "build": "npm run install-ci && npm run build:stencil && npm run build:styling",
        "build:stencil": "stencil build --prod",
        "build:styling": "npx tailwindcss -i ./Frontend/styling/app.css -o ./wwwroot/css/app.css --minify",
        "start": "concurrently --names \"STENCIL,STYLING\" --prefix-colors \"cyan.bgBlue.bold,red.bgRed.bold\" \"stencil build --dev --watch\" \"npm run start:styling\"",
        "start:stencil": "stencil build --dev --watch",
        "start:styling": "npx tailwindcss -i ./Frontend/styling/app.css -o ./wwwroot/css/app.css --watch",
        "pretest": "tsc --project tsconfig.tests.json",
        "test": "npm run test:frontend && npm run test:stencil",
        "test:stencil": "stencil test --roots=\"Frontend/scripts/src/components\" --spec --e2e --ci --reporters='default' --reporters='jest-junit' --detectOpenHandles",
        "test:frontend": "jest --roots=\"Frontend/scripts/src/utils\" --ci --reporters='default' --reporters='jest-junit'",
        "install-ci": "yarn install",
        "pre-build": "npm install -g yarn && npm run build",
        "storybook": "start-storybook --port 1338 --config-dir ./Frontend/storybook/config/dev --static-dir ./Frontend/storybook/dist --quiet"
    },
    "devDependencies": {
        "@stencil/core": "^2.18.1",
        "@stencil/eslint-plugin": "^0.4.0",
        "@stencil/store": "^2.0.1",
        "@stencil/stylus": "^1.0.1",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/addon-viewport": "^6.5.12",
        "@storybook/addons": "^6.5.12",
        "@storybook/core": "^6.5.12",
        "@storybook/theming": "^6.5.12",
        "@storybook/web-components": "^6.5.12",
        "@types/jest": "26.0.22",
        "@types/puppeteer": "5.4.4",
        "@typescript-eslint/eslint-plugin": "^5.30.3",
        "@typescript-eslint/parser": "^5.30.3",
        "concurrently": "^7.2.2",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-webpack-plugin": "^3.2.0",
        "hygen": "^6.2.11",
        "jest": "^28.1.2",
        "jest-cli": "^28.1.2",
        "jest-junit": "^14.0.0",
        "lint-staged": "^13.0.3",
        "lit-html": "^2.2.6",
        "miragejs": "^0.1.45",
        "prettier": "2.7.1",
        "puppeteer": "^15.3.0",
        "tailwindcss": "^3.1.8",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "workbox-build": "6.5.3"
    },
    "jest-junit": {
        "outputDirectory": "./test-results"
    },
    "dependencies": {
        "storybook": "^6.3.8"
    },
    "-vs-binding": {
        "ProjectOpened": [
            "build"
        ]
    }
}
