{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "static:storybook": "concurrently --names \"STORYBOOK,STATIC\" --prefix-colors \"yellow.bgMagenta.bold,cyan.bgBlue.bold\" \"npm run storybook\" \"npm run static\"",
    "build": "npm run install-ci && npm run build:stencil",
    "build:stencil": "cd Frontend/scripts && npm run build",
    "start": "cd Frontend/scripts && npm run start",
    "static": "cd Frontend/scripts && npm run static",
    "test": "cd Frontend/scripts && npm run test",
    "generate": "cd Frontend/scripts && npm run generate",
    "install-ci": "yarn install && npm run install:stencil",
    "install:stencil": "cd Frontend/scripts && yarn install",
    "pre-build": "npm install -g yarn && npm run build",
    "storybook": "start-storybook --port 5014 --config-dir ./Frontend/storybook/config/dev --static-dir ./Frontend/storybook/dist --quiet"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/core": "^6.3.7",
    "@storybook/theming": "^6.3.7",
    "@storybook/web-components": "^6.3.7",
    "@types/jest": "26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.0.1",
    "hygen": "^6.1.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "lint-staged": "^11.1.2",
    "lit-html": "^1.4.1",
    "miragejs": "^0.1.41",
    "prettier": "2.3.2"
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  },
  "lint-staged": {
    "frontend/scripts/**/*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "storybook": "^6.3.8"
  }
}
