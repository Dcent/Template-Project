{
  "name": "stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil/stencil.esm.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prod",
    "start": "stencil build --dev --watch --serve",
    "static": "stencil build --dev --watch --static",
    "test": "stencil test --spec --e2e --ci --reporters='jest-junit' --detectOpenHandles",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  },
  "dependencies": {
    "@stencil/core": "2.8.1",
    "rating-stencil-component": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/store": "^1.4.1",
    "@stencil/stylus": "^1.0.1",
    "@types/puppeteer": "5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^26.6.3",
    "jest-cli": "26.6.3",
    "jest-junit": "^12.2.0",
    "puppeteer": "^10.2.0",
    "workbox-build": "6.2.4"
  },
  "license": "MIT"
}
